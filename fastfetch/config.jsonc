{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "$(find \"${XDG_CONFIG_HOME:-$HOME/.config}/fastfetch/pngs/\" -name \"*.png\" | shuf -n 1)",
    "height": 18
  },
  "display": {
    "separator" : " : "
  },
  "modules": [
    {
      "type": "title",
      "key": "  ",
      "format": "{6} {7} {8}"
    },
    {
      "type": "custom",
      "format": "┌──────────────────────────────────────────┐"
    },
    {
      "type": "os",
      "key": "   ",
      "format": "{2}",
      "keyColor": "blue"
    },
    {
      "type": "host",
      "key": "   ",
      "format": ""
    },
    {
      "type": "wm",
      "key": "  󱗃 ",
      "format": "{2}",
      "keyColor": "yellow"
    },
    {
      "type": "terminal",
      "key": "  >_",
      "keyColor": "yellow"
    },{
      "type": "packages",
      "key": "  󰏗 ",
      "keyColor": "green"
    },
    {
      "type": "display",
      "key": "  󰍹 ",
      "format": "{1}x{2} @ {3}Hz [{7}]",
      "keyColor": "green"
    },  {
      "type": "kernel",
      "key": "   ",
      "format": "{2}",
      "keyColor": "red"
    },  {
      "type": "chassis",
      "key": "  󰇺 ",
      "format": "{3}"
    },
    {
      "type": "custom",
      "format": "└──────────────────────────────────────────┘"
    },
    {
      "type": "custom",
      "format": "┌──────────────────────────────────────────┐"
    },
    {
      "type": "cpu",
      "format": "{1} ", //@ {7}",
      "key": "   ",
      "keyColor": "blue"
    },
    {
      "type": "gpu",
      "format": "{1} {2}",
      "key": "  󰊴 ",
      "keyColor": "blue"
    },
    {
      "type": "disk",
      "key": "  🖴 "
    },
    {
      "type": "memory",
      "key": "   ",
      "keyColor": "magenta"
    },
    {
      "type": "uptime",
      "key": "  󱫐 ",
      "keyColor": "red"
    },
    {
      "type": "battery",
      "key": "  ⚡",
      "keyColor": "blink_"
    },
    {
      "type": "custom",
      "format": "└──────────────────────────────────────────┘"
    },
    {
      "type": "colors",
      "paddingLeft": 2,
      "symbol": "circle"
    },
    "break"
  ]
}
